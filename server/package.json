      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "express": "^4.16.4",
    "jsonfile": "^5.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.4",
    "multer": "^1.4.1",
    "socket.io": "^2.2.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jsonfile": "^5.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.3.26",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.11.3",
    "@types/socket.io": "^2.1.2",
    "nodemon": "^1.18.9"
